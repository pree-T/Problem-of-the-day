class Solution {
public:
    string reverseVowels(string s) {
        int n=s.size();
        int i=0;
        int j=n-1;
        
        while(i<j)
        {
            while(s[i]!='a' && s[i]!='A' && s[i]!='e' && s[i]!='E' || s[i]!='I' || s[i]!='i'||
              s[i]!='o'|| s[i]!='O'|| s[i]!='u'|| s[i]!='U')
                ++i;
            
        }
        
    }
};
